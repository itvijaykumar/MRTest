
<script type="text/html" id="generic-adjustedentries-template">
    <div id="toolTip"></div>
    <div class="row" id="timesheetEntries">

        @*data-bind="with:ParentModel">*@
        <div class="ts-table">
            <div class="col-md-12" data-bind="with:ChildModel.Timesheet">
                <div id="timesheetTable" class="table-responsive">

                    <table id="editTimesheet" class="table-condensed table-hover">
                        <tr style="height: 1px;">
                            <th class="col1">
                                Day
                            </th>
                            <th class="col2">
                                Attendance
                            </th>
                            <th class="col3" style="width: 85px !important;">
                                Time Worked <br />
                                <label style="padding-left: 12px;"> hh : mm</label>
                            </th>
                            <th class="col4" style="width: 45px !important;" data-bind="visible : OvertimeVisibilty ">
                                &nbsp; Overtime<br />
                                <label>hh : mm</label>
                            </th>
                            <th class="col5" data-bind="visible : OverProductionVisibilty ">
                                Over Production
                            </th>
                            <th class="col6">
                                One Off Payment
                            </th>
                            <th class="col7" data-bind="visible : AccomodationVisibilty ">
                                Accommodation
                            </th>
                            <th class="col8" data-bind="visible : MealAllowanceVisibilty ">
                                Meal Allowance
                            </th>
                            <th class="col9" data-bind="visible : TravelVisibilty ">
                                Travel
                            </th>
                            <th class="col10" data-bind="visible : MileageVisibilty ">
                                No of<br /> Miles
                            </th>
                            <th class="col11" data-bind="visible : MileageVisibilty ">
                                Mileage at
                                <span data-bind="text: MileageLimit"></span>p per mile
                            </th>
                            <th class="col12" data-bind="visible : ParkingVisibilty ">
                                Parking
                            </th>
                            <th class="col13" data-bind="visible : OtherVisibilty ">
                                Other
                            </th>
                        </tr>
                        <tbody data-bind="foreach: Entries()">

                            <tr>
                                <td class="col1">
                                    <span data-bind="text: $parents[1].ParentModel.Timesheet().Entries()[$index()].Day"></span>
                                </td>
                                <td class="col2">
                                    <select class="att-row col2 " data-bind="enable: false, options: $parents[1].ParentModel.Timesheet().Entries()[$index()].Attendences, optionsText: 'AttendenceOption', optionsValue:'AttendenceOptionId' , value: $parents[1].ParentModel.Timesheet().Entries()[$index()].AttendenceId"></select>
                                </td>

                                <td class="col3">
                                    <input type="text" class="k-textbox ts-hour-min" style="width: 30px" data-bind="value: $parents[1].ParentModel.Timesheet().Entries()[$index()].ActualHour, enable: false " />
                                    <label style="padding: 5px 2px; margin: 0;">:</label>
                                    <input type="text" class="ts-hour-min k-textbox" style="width: 30px" data-bind="value: $parents[1].ParentModel.Timesheet().Entries()[$index()].ActualMinute,enable: false" />
                                </td>

                                <td class="col4" data-bind="visible : $parent.OvertimeVisibilty ">
                                    <input type="text" class="k-textbox ts-hour-min overtime-hour" style="width: 30px" data-bind="value: $parents[1].ParentModel.Timesheet().Entries()[$index()].ActualOvertimeHour, enable: false" />
                                    <label style="padding: 5px 2px; margin: 0;">:</label>
                                    <input type="text" class="k-textbox ts-hour-min overtime-minute" style="width: 30px" data-bind="value: $parents[1].ParentModel.Timesheet().Entries()[$index()].ActualOvertimeMinute, enable: false" />
                                </td>
                                <td class="col5" data-bind="visible : $parent.OverProductionVisibilty">
                                    <input type="text" style="width: 45px;" class="k-textbox" data-bind="value: $parents[1].ParentModel.Timesheet().Entries()[$index()].OverProduction, enable: false" />

                                </td>
                                <td class="col6">
                                    <input type="text" style="width: 45px" class="k-textbox" data-bind="value: $parents[1].ParentModel.Timesheet().Entries()[$index()].OneOfPayment, enable: false " />

                                </td>
                                <td class="col7" data-bind="visible : $parent.AccomodationVisibilty ">
                                    <input type="text" style="width: 45px" class="k-textbox" data-bind="value: $parents[1].ParentModel.Timesheet().Entries()[$index()].Accomodation, enable: false" />
                                    <span data-bind="event : {mouseover : function(data,event) {ShowMe($parents[1].ParentModel.Timesheet().Entries()[$index()].AccomodationNotes,data, event)}, mouseout : HideMe }"><span data-bind="ToggleElem:$parents[1].ParentModel.Timesheet().Entries()[$index()].AccomodationNotes" class="glyphicon glyphicon-comment" /> </span>

                                </td>
                                <td class="col8" data-bind="visible : $parent.MealAllowanceVisibilty ">
                                    <input type="text" style="width: 45px" class="k-textbox" class="k-textbox" data-bind="value: $parents[1].ParentModel.Timesheet().Entries()[$index()].MealAllowance, enable: false" />
                                    <span data-bind="event : {mouseover : function(data,event) {ShowMe($parents[1].ParentModel.Timesheet().Entries()[$index()].MealAllowanceNotes,data, event)}, mouseout : HideMe }"><span data-bind="ToggleElem:$parents[1].ParentModel.Timesheet().Entries()[$index()].MealAllowanceNotes" class="glyphicon glyphicon-comment" /> </span>

                                </td>
                                <td class="col9" data-bind="visible : $parent.TravelVisibilty ">
                                    <input type="text" style="width: 45px" class="k-textbox" class="k-textbox" data-bind="value: $parents[1].ParentModel.Timesheet().Entries()[$index()].Travel, enable: false" />
                                    <span data-bind="event : {mouseover : function(data,event) {ShowMe($parents[1].ParentModel.Timesheet().Entries()[$index()].TravelNotes,data, event)}, mouseout : HideMe }"><span data-bind="ToggleElem:$parents[1].ParentModel.Timesheet().Entries()[$index()].TravelNotes" class="glyphicon glyphicon-comment" /> </span>
                                </td>
                                <td class="col10" data-bind="visible : $parent.MileageVisibilty ">
                                    <input type="text" style="width: 45px" class="k-textbox" data-bind="value: $parents[1].ParentModel.Timesheet().Entries()[$index()].NoOfMiles, enable: false" />
                                    <span data-bind="event : {mouseover : function(data,event) {ShowMe($parents[1].ParentModel.Timesheet().Entries()[$index()].NoOfMilesNotes,data, event)}, mouseout : HideMe }"><span data-bind="ToggleElem:$parents[1].ParentModel.Timesheet().Entries()[$index()].NoOfMilesNotes" class="glyphicon glyphicon-comment" /></span>

                                </td>
                                <td class="col11" data-bind="visible : $parent.MileageVisibilty ">
                                    <input type="text" style="width: 45px;" class="k-textbox" data-bind="value: $parents[1].ParentModel.Timesheet().Entries()[$index()].Mileage, enable: false" />
                                </td>
                                <td class="col12" data-bind="visible : $parent.ParkingVisibilty ">
                                    <input type="text" style="width: 45px" class="k-textbox" data-bind="value: $parents[1].ParentModel.Timesheet().Entries()[$index()].Parking, enable: false" />
                                    <span data-bind="event : {mouseover : function(data,event) {ShowMe($parents[1].ParentModel.Timesheet().Entries()[$index()].ParkingNotes,data, event)}, mouseout : HideMe }"><span data-bind="ToggleElem:$parents[1].ParentModel.Timesheet().Entries()[$index()].ParkingNotes" class="glyphicon glyphicon-comment" /> </span>
                                </td>
                                <td class="col13" data-bind="visible : $parent.OtherVisibilty ">
                                    <input type="text" style="width: 45px" class="k-textbox" data-bind="value: $parents[1].ParentModel.Timesheet().Entries()[$index()].Other, enable: false" />
                                    <span data-bind="event : {mouseover : function(data,event) {ShowMe($parents[1].ParentModel.Timesheet().Entries()[$index()].OtherNotes,data, event)}, mouseout : HideMe }"><span data-bind="ToggleElem:$parents[1].ParentModel.Timesheet().Entries()[$index()].OtherNotes" class="glyphicon glyphicon-comment" /></span>
                                </td>
                            </tr>


                            <tr style="background-color: #FFD6AE; ">
                                <td class="col1">
                                    <span>Adjustment</span>
                                    <input type="hidden" class="hdnTSEntry" data-bind="value: TimeSheetEntryId" />
                                </td>
                                <td>
                                    <select class="att-row col2 " data-bind="enable: IsDay && $parent.Enable, DisableElem:EnableAbsense, options: Attendences, optionsText: 'AttendenceOption', optionsValue:'AttendenceOptionId' , value: AttendenceId"></select>

                                </td>
                                <td class="col3">
                                    <input type="text" class="k-textbox ts-hour-min" style="width: 30px" data-bind="value: ActualHour, DisableElem: EnableHour " />
                                    <label style="padding: 5px 2px; margin: 0;">:</label>
                                    <input type="text" class="ts-hour-min k-textbox" style="width: 30px" data-bind="value: ActualMinute,DisableElem:EnableHour" />
                                </td>

                                <td class="col4" data-bind="visible : $parent.OvertimeVisibilty ">
                                    <input type="text" style="width: 45px" class="actual-overtime hidden" data-bind="value: OverTime">
                                    <input type="text" class="k-textbox ts-hour-min overtime-hour" style="width: 30px" data-bind="value: ActualOvertimeHour, DisableElem: EnableHour && IsOvertime, event : {change : function(data,event) {UpdateOverTime(data, event)} }" />
                                    <label style="padding: 5px 2px; margin: 0;">:</label>
                                    <input type="text" class="k-textbox ts-hour-min overtime-minute" style="width: 30px" data-bind="value: ActualOvertimeMinute, DisableElem: EnableHour && IsOvertime, event : {change : function(data,event) {UpdateOverTime(data, event)} }" />
                                </td>
                                <td class="col5" data-bind="visible : $parent.OverProductionVisibilty">
                                    <input type="text" style="width: 45px;" class="k-textbox" data-bind="value: OverProduction, DisableElem: true" />

                                </td>
                                <td class="col6">
                                    <input type="text" style="width: 45px" class="k-textbox" data-bind="value: OneOfPayment, DisableElem: true " />

                                </td>
                                <td class="col7" data-bind="visible : $parent.AccomodationVisibilty ">
                                    <input type="text" style="width: 45px" class="k-textbox" data-bind="value: Accomodation, DisableElem:true" />
                                    <a class="links" href="javascript:void(0)" data-bind="editable: AccomodationNotes, event : {mouseover : function(data,event) {ShowMe(AccomodationNotes,data, event)}, mouseout : HideMe } , jqyValidate : this ,editableOptions: { type: 'textarea', display: false, disabled : !(IsDay && $parent.Enable)}"><span data-bind="ToggleElem:AccomodationNotes" class="glyphicon glyphicon-pencil" /><span data-bind="ToggleElem:AccomodationNotes" class="glyphicon glyphicon-comment" /> </a>

                                </td>
                                <td class="col8" data-bind="visible : $parent.MealAllowanceVisibilty ">
                                    <input type="text" style="width: 45px" class="k-textbox" class="k-textbox" data-bind="value: MealAllowance, DisableElem:true" />
                                    <a class="links" href="javascript:void(0)" data-bind="editable: MealAllowanceNotes, event : {mouseover : function(data,event) {ShowMe(MealAllowanceNotes,data, event)}, mouseout : HideMe } ,editableOptions: { type: 'textarea', display: false, disabled : !(IsDay && $parent.Enable)}"><span data-bind="ToggleElem:MealAllowanceNotes" class="glyphicon glyphicon-pencil"></span><span data-bind="ToggleElem:MealAllowanceNotes" class="glyphicon glyphicon-comment" /> </a>

                                </td>
                                <td class="col9" data-bind="visible : $parent.TravelVisibilty ">
                                    <input type="text" style="width: 45px" class="k-textbox" class="k-textbox" data-bind="value: Travel, DisableElem:true" />
                                    <a class="links" href="javascript:void(0)" data-bind="editable: TravelNotes, event : {mouseover : function(data,event) {ShowMe(TravelNotes,data, event)}, mouseout : HideMe } ,editableOptions: { type: 'textarea', display: false, disabled : !(IsDay && $parent.Enable)}"><span data-bind="ToggleElem:TravelNotes" class="glyphicon glyphicon-pencil"></span> <span data-bind="ToggleElem:TravelNotes" class="glyphicon glyphicon-comment" /> </a>
                                </td>
                                <td class="col10" data-bind="visible : $parent.MileageVisibilty ">
                                    <input type="text" style="width: 45px" class="k-textbox" data-bind="value: NoOfMiles, DisableElem:true" />
                                    <a class="links" href="javascript:void(0)" data-bind="editable: NoOfMilesNotes,event : {mouseover : function(data,event) {ShowMe(NoOfMilesNotes,data, event)}, mouseout : HideMe } , editableOptions: { type: 'textarea', display: false, disabled : !(IsDay && $parent.Enable)}"><span data-bind="ToggleElem:NoOfMilesNotes  " class="glyphicon glyphicon-pencil"></span><span data-bind="ToggleElem:NoOfMilesNotes" class="glyphicon glyphicon-comment" /></a>

                                </td>
                                <td class="col11" data-bind="visible : $parent.MileageVisibilty ">
                                    <input type="text" style="width: 45px;" class="k-textbox" data-bind="value: Mileage, enable: false" />
                                </td>
                                <td class="col12" data-bind="visible : $parent.ParkingVisibilty ">
                                    <input type="text" style="width: 45px" class="k-textbox" data-bind="value: Parking, DisableElem:true" />
                                    <a class="links" href="javascript:void(0)" data-bind="editable: ParkingNotes,event : {mouseover : function(data,event) {ShowMe(ParkingNotes,data, event)}, mouseout : HideMe } , editableOptions: { type: 'textarea', display: false, disabled : !(IsDay && $parent.Enable) }"><span data-bind="ToggleElem:ParkingNotes" class="glyphicon glyphicon-pencil"></span> <span data-bind="ToggleElem:ParkingNotes" class="glyphicon glyphicon-comment" /> </a>
                                </td>
                                <td class="col13" data-bind="visible : $parent.OtherVisibilty ">
                                    <input type="text" style="width: 45px" class="k-textbox" data-bind="value: Other, DisableElem:true" />
                                    <a class="links" href="javascript:void(0)" data-bind="editable: OtherNotes,event : {mouseover : function(data,event) {ShowMe(OtherNotes,data, event)}, mouseout : HideMe } , editableOptions: { type: 'textarea', display: false, disabled : !(IsDay && $parent.Enable) }"><span data-bind="ToggleElem:OtherNotes" class="glyphicon glyphicon-pencil" /> <span data-bind="ToggleElem:OtherNotes" class="glyphicon glyphicon-comment" /></a>
                                </td>
                            </tr>


                        </tbody>
                        <tr style="border-right-style:hidden;border-left-style:hidden; ">
                            <td colspan="13"></td>
                        </tr>

                        <tr data-bind="visible: ApproveNotAllowed">
                            <td class="col1" style="">
                                <span> Incentive days</span>
                            </td>
                            <td colspan="12">
                                <input type="text" data-bind="value: $parent.ChildModel.IncentiveDays" style="width: 45px" />
                            </td>
                        </tr>
                        <tr>
                            <td class="col1" style="text-align:center; vertical-align:middle;">
                                <span>Adjustment Totals</span>
                            </td>
                            <td class="col2">
                                <label>£ </label>
                                <span data-bind="text: $parent.ChildModel.TotalWorking" />
                            </td>
                            <td class="col3">
                                <label>&nbsp;</label>
                                <span data-bind="text: $parent.ChildModel.TotalWorkingHours" style="width: 30px" />
                            </td>
                            <td class="col4" data-bind="visible : OvertimeVisibilty ">
                                <label>£</label>
                                <span data-bind="text: $parent.ChildModel.TotalOverTime" style="width: 30px" />

                            </td>
                            <td class="col5" data-bind="visible : OverProductionVisibilty ">
                                <label>£</label>
                                <span data-bind="text: $parent.ChildModel.TotalOverProduction" style="width: 45px;" />

                            </td>
                            <td class="col6">
                                <label>£</label>
                                <span data-bind="text: $parent.ChildModel.TotalOneOfPayment" style="width: 45px" />

                            </td>
                            <td class="col7" data-bind="visible : AccomodationVisibilty ">
                                <label>£</label>
                                <span data-bind="text: $parent.ChildModel.TotalAccomodation" />

                            </td>
                            <td class="col8" data-bind="visible : MealAllowanceVisibilty ">
                                <label>£</label>
                                <span data-bind="text: $parent.ChildModel.TotalMealAllowance" />

                            </td>
                            <td class="col9" data-bind="visible : TravelVisibilty ">
                                <label>£</label>
                                <span data-bind="text: $parent.ChildModel.TotalTravel" />

                            </td>
                            <td class="col10" data-bind="visible : MileageVisibilty ">
                                <label>&nbsp;</label>
                                <span data-bind="text: $parent.ChildModel.TotalNoOfMiles" />
                            </td>
                            <td class="col11" data-bind="visible : MileageVisibilty ">
                                <label>£</label>
                                <span data-bind="text: $parent.ChildModel.TotalMileage" />
                            </td>
                            <td class="col12" data-bind="visible : ParkingVisibilty ">
                                <label>£</label>
                                <span data-bind="text: $parent.ChildModel.TotalParking" />

                            </td>
                            <td class="col13" data-bind="visible : OtherVisibilty ">
                                <label>£</label>
                                <span data-bind="text: $parent.ChildModel.TotalOther" />
                            </td>
                        </tr>
                    </table>
                </div>
            </div>
            <br />
            <div class="col-md-8 pull-right" id="newComment" data-bind="visible: ChildModel.IsComment">
                <h3>New Comment</h3>
                <div>
                    <textarea id="commentText" data-bind="enable: ChildModel.CanReject" rows="2" cols="40" style="width: 320px;"></textarea>
                    <strong><label style="color:tomato; margin-left: 10px " id="rejectCommentLabel"></label></strong>
                </div>
            </div>
            <div class="col-md-5 pull-left" id="historyTable" data-bind="visible: ChildModel.EnableHistory">
                <h3>History</h3>
                <table class="table table-condensed table-hover ">
                    <tr style="height: 1px;">
                        <th>Date</th>
                        <th>Time</th>
                        <th>Name</th>
                        <th>Status</th>
                        <th>Comment</th>
                    </tr>
                    <tbody data-bind="foreach: ChildModel.Timesheet().History">
                        <tr>
                            <td>
                                <span data-bind="text: Date"></span>
                            </td>
                            <td>
                                <span data-bind="text: Time"></span>
                            </td>
                            <td>
                                <span data-bind="text: Associate"></span>
                            </td>
                            <td>
                                <span data-bind="text: Status"></span>
                            </td>
                            <td>
                                <span data-bind="text: Comment"></span>
                            </td>
                        </tr>
                    </tbody>
                </table>
            </div>

            <div style="width:965px !important; clear: left;">
                <div id="timesheetExpenseReciepts">
                    <h3>Expense Receipts<img src="../../Content/images/info.png" id="expense-receipt" /> </h3>
                    <div id="expenseReceiptUpload" data-bind="visible: ChildModel.CanUploadReceipt">
                        <div style=" display inline-block; width 180px;">
                            <div class="editor-label">
                                <label for="expenseName" class="field-required">Expense title</label>
                            </div>
                            <div class="editor-field">
                                <input type="text" id="AexpenseName" class="k-textbox" />
                            </div>
                        </div>

                        <div style="display: inline-block; width: 180px;">
                            <div class="editor-label">
                                <label for="expenseDate" class="field-required">Expense Receipt Date</label>
                            </div>
                            <div class="editor-field">
                                <div style="width: 150px;">
                                    <input class="datepicker ignore" data-bind="kendoDatePicker: { value: null, format: 'dd/MM/yyyy' } " id="AexpenseDate" name="AexpenseDate" placeholder="dd/mm/yyyy" type="text" />
                                </div>
                            </div>
                        </div>

                        <div style="display: inline-block; width: 100px;">

                            <button style="margin-top: 20px; margin-bottom: 20px; margin-left: 54px;" class="btn-slim submit-button"
                                    data-bind="click: function(){ChildModel.SaveOnExpenseLoad();UploadTimeSheetExpense(this, ChildModel.Timesheet().TimesheetId); return false;}">
                                Upload
                            </button>
                        </div>
                    </div>

                    <br />
                    <div class="clear"></div>
                    <br />

                    <div id="AreceiptsGrid" style="margin-bottom: 10px;"></div>
                    <button type="button" class="submit-button btn-slim" onclick="javascript:AcceptSelectedReceipts();" data-bind="visible:ChildModel.IsBulkLoad">Accept Selected</button>
                    <button type="button" class="submit-button btn-slim" onclick="javascript:RejectSelectedReceipts();" data-bind="visible:ChildModel.IsBulkLoad">Reject Selected</button>

                </div>
            </div>

            <br />
            <div class="clear"></div>
            <br />

            <div class="buttons-container">

                <div class="col-md-8">
                    <div class="btn-group pull-right" data-bind="visible: ChildModel.CanApprove">
                        <button class="btn-save" data-bind="click: ChildModel.Approve , enable: ChildModel.ApproveEnabled">Approve</button>
                    </div>
                    <div class="btn-group pull-right" data-bind="visible: ChildModel.CanReject">
                        <button class="btn-save" data-bind="click: ChildModel.Reject, enable: ChildModel.RejectEnabled">Reject</button>
                    </div>

                    <div class="col-md-8" data-bind="visible: ChildModel.RejectToUpdate">

                    </div>
                </div>

                <div class="col-md-8 pull-right" style="text-align: right;">
                    <div class="btn-group" data-bind="visible: ChildModel.CanPending">
                        <button class="btn-save" data-bind="click: ChildModel.Pending">Pending Receipts</button>
                    </div>
                    <div class="btn-group" data-bind="visible: ChildModel.CanComplete">
                        <button class="btn-save" data-bind="click: ChildModel.Complete">Checked</button>
                    </div>
                    @*<div class="btn-group" data-bind="visible: ChildModel.CanAdjust">
                            <button class="btn-save" data-bind="click:ChildModel.Adjustment ">Adjustment</button>
                        </div>*@
                </div>


                <div class="col-md-8 pull-right">

                    <div class="btn-group pull-right" data-bind="visible: ChildModel.CanSubmit">
                        <button class="btn-save" data-bind="click: ChildModel.Submit , enable: ChildModel.SubmitEnabled">Submit</button>
                    </div>

                    <div class="btn-group pull-right" data-bind="visible: ChildModel.CanSave">
                        <button class="btn-save" data-bind="click: ChildModel.Save, enable: ChildModel.SaveEnabled">Save</button>
                    </div>

                    <div class="btn-group pull-right" data-bind="visible: ChildModel.RejectToUpdate">
                        <button class="btn-save" data-bind="click: ChildModel.AssociateReject, enable: ChildModel.CanReject">Reject</button>
                    </div>

                    <div class="btn-group pull-right" data-bind="visible: ChildModel.IsRejected && ChildModel.CanReSubmit">
                        <button class="btn-save" data-bind="click: ChildModel.ReSubmit, enable: ChildModel.ReSubmitEnabled">Re-Submit</button>
                    </div>
                </div>

            </div>

            <div id="rejectDialog" class="" data-bind=" visible:false"></div>

        </div>

        <div class="col-md-6">
            <div class="btn-group pull-left">
                <button class="btn-save" data-bind="click: ChildModel.Back">Back</button>
            </div>
        </div>

    </div>
</script>
